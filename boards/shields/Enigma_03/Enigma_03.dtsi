#include <physical_layouts.dtsi>

/ {
    enigma_layout: enigma_layout {
        compatible = "zmk,physical-layout";
        display-name = "Enigma_03_layout";
        transform = <&default_transform>;
        kscan = <&kscan0>;

        keys = <
            &key_physical_attrs 100 100   0 0 0 0 0    // RC(0,0)
            &key_physical_attrs  50  50 100 0 0 0 0    // RC(0,1)
            &key_physical_attrs 100 100 150 0 0 0 0    // RC(0,2)
        >;
    };
};